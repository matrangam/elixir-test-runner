{
	"name": "elixir-test-runner",
	"description": "Runs tests in exs files inside vscode",
	"version": "0.0.1",
	"publisher": "matrangam",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"repository": {
		"url": "https://github.com/matrangam/elixir-test-runner"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:elixir"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "exlixir_test_runner.run_test",
				"description": "Background decoration color for lines that can execute a test",
				"defaults": {
					"dark": "#ff0000b8",
					"light": "#ff0000b8",
					"highContrast": "#ff0000b8"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc  -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^8.9.1",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"@types/vscode": "^1.32.0"
	}
}
